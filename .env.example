# Environment variables for CareBot application
# Copy this file to .env and set your actual tokens

# Telegram Bot Tokens (REQUIRED)
TELEGRAM_BOT_TOKEN=your_production_bot_token_here
TELEGRAM_BOT_TOKEN_TEST=your_test_bot_token_here

# Test Mode Configuration
CAREBOT_TEST_MODE=false

# Server configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=5555

# Database configuration
DATABASE_PATH=/app/data/game_database.db

# Application settings
DEBUG=False
LOG_LEVEL=INFO